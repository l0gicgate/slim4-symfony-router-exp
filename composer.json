{
    "name": "l0gicgate/slim4-symfony-router-exp",
    "description": "Slim 4 Symfony Router Experiment",
    "type": "library",
    "require": {
        "php": "^7.4",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "symfony/routing": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "l0gicgate",
            "email": "pierre@lgse.com"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Slim\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slim\\Tests\\": "tests/"
        }
    }
}
